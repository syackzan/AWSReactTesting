{
  "function": {
    "igloofunction": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "vanceJoySongStorage",
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ]
        },
        {
          "category": "storage",
          "resourceName": "iglooStorage",
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ]
        },
        {
          "category": "storage",
          "resourceName": "britneySongStorage",
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ]
        }
      ]
    }
  },
  "api": {
    "iglooapi": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "igloofunction",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  },
  "storage": {
    "vanceJoySongStorage": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    },
    "iglooStorage": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    },
    "britneySongStorage": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  }
}